{
  "version": 2,
  "builds": [
    {
      "src": "uploads//*",
      "use": "@vercel/static"
    },
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/uploads/$1"
    }
  ]
}
